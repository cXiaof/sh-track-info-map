  "scripts": {
    "build": "craco build",
    "day": "npm run geo:track && craco build",
    "format": "prettier --loglevel silent --write \"src/**/*.{html,css,scss,less,js,jsx,ts,tsx,md,json}\"",
    "format:geojson": "prettier --loglevel silent --write \"public/data/track.geojson\"",
    "geo:risk": "node geo/risk.js",
    "geo:track": "node geo/track.js && npm run format:geojson",
    "prepublish": "npm run format",
    "start": "craco start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "ahooks": "^3.4.1",
    "antd-mobile": "^5.13.1",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.3",
    "lodash": "^4.17.21",
    "number-precision": "^1.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.41",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.7",
    "fs-extra": "^10.1.0",
    "node-sass": "^7.0.1",
    "normalize.css": "^8.0.1",
    "open-color": "^1.9.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-packagejson": "^2.2.18",
    "react-app-alias": "^2.2.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.3"
  }
}
