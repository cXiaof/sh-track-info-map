{
  "name": "sh-track-info-map",
  "version": "0.3.4",
  "homepage": ".",
  "license": "Apache",
  "author": "cXiaof",
  "scripts": {
    "build": "craco build",
    "day": "npm run geo:risk && npm run geo:track && craco build",
    "format": "prettier --loglevel silent --write \"src/**/*.{html,css,scss,less,js,jsx,ts,tsx,md,json}\"",
    "format:geojson": "prettier --loglevel silent --write \"public/data/track.geojson\"",
    "geo:risk": "node geo/risk.js",
    "geo:track": "node geo/track.js && npm run format:geojson",
    "prepublish": "npm run format",
    "start": "craco start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "ahooks": "^3.7.1",
    "antd-mobile": "^5.23.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.5",
    "lodash": "^4.17.21",
    "number-precision": "^1.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.5",
    "@types/lodash": "^4.14.185",
    "@types/node": "^17.0.43",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.11",
    "fs-extra": "^10.1.0",
    "normalize.css": "^8.0.1",
    "open-color": "^1.9.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "react-app-alias": "^2.2.2",
    "sass": "^1.54.9",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.6.4"
  }
}
