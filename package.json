{
  "name": "sh-track-info-map",
  "version": "0.4.3",
  "homepage": "https://cxiaof.github.io/sh-track-info-map",
  "license": "Apache",
  "author": "cXiaof",
  "scripts": {
    "build": "craco build",
    "day": "npm run geo:risk && npm run geo:track",
    "format": "prettier --loglevel silent --write \"src/**/*.{html,css,scss,less,js,jsx,ts,tsx,md,json}\"",
    "format:geojson": "prettier --loglevel silent --write \"public/data/track.geojson\"",
    "geo:risk": "node geo/risk.js",
    "geo:track": "node geo/track.js && npm run format:geojson",
    "prepublish": "npm run format",
    "start": "craco start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "ahooks": "^3.7.2",
    "antd-mobile": "^5.25.1",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.6",
    "lodash": "^4.17.21",
    "number-precision": "^1.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.5",
    "@types/lodash": "^4.14.188",
    "@types/node": "^17.0.43",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "fs-extra": "^10.1.0",
    "normalize.css": "^8.0.1",
    "open-color": "^1.9.1",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "react-app-alias": "^2.2.2",
    "sass": "^1.56.1",
    "tailwindcss": "^3.2.2",
    "typescript": "~4.6.4"
  }
}
